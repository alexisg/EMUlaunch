// © Ivan Dembicki, dembicki@narod.ruMovieClip.prototype.getTopDepth = function() { var mc, mcd, d; for (mcd in this) {  mc = this[mcd];  if (mc._name == mcd & typeof mc.getDepth === "function" & mc._parent === this) {   d = this[mcd].getDepth()+1;   if (d<1) {return 1} else {return d;}}}return 1;};MovieClip.prototype.RECT_ROUND = function(w, h, radius, fStyle, lStyle, mcName, mcDepth) {	if (mcDepth == undefined) mcDepth = this.getTopDepth();	trace(mcDepth)	if (mcName == undefined) mcName = "_$rect" + mcDepth;	trace(mcName);	if (radius == undefined) radius = 0;	if (w == undefined) w = 100;	if (h == undefined) h = 100;	if ((lStyle == undefined) && (fStyle==undefined)){		lStyle = new Object();		lStyle.thickness = 1;		lStyle.col = 0x000000;		lStyle.alph = 100;	}		var rect = this.createEmptyMovieClip(mcName, mcDepth);	with (rect) {		lineStyle(lStyle.thickness, lStyle.col, lStyle.alph);		if (fStyle != undefined) beginFill(fStyle.col, fStyle.alph);		moveTo(radius, 0);		lineTo(w - radius, 0);		curveTo(w, 0, w, radius);		lineTo(w, h - radius);		curveTo(w, h, w - radius, h);		lineTo(radius, h);		curveTo(0, h, 0, h - radius);		lineTo(0, radius);		curveTo(0, 0, radius, 0);	}};this.myFillStyle = new Object();this.myFillStyle.alph = 85;this.myFillStyle.col = 0x000000;